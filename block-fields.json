{
    "name": "My Controllers",
    "output": "./blocks",
    "toggles": {
        "bio": {
            "title": "Bio data",
            "isOpen": true,
            "icon": ""
        },
        "occuption": {
            "title": "Occuption",
            "isOpen": false
        }
    },
    "fields": [
        {
            "title": "Your Name",
            "id": "your-name",
            "type": "text",
            "class": "your-name",
            "toggle": "bio",
            "attributeName": "name",
            "baseControl": true
        },
        {
            "title": "Accept t&c",
            "id": "terms",
            "label": "Sample",
            "toggle": "bio",
            "checked": true,
            "type": "checkbox"
        },
        {
            "title": "Save Changes",
            "id": "save-changes",
            "type": "button",
            "toggle": "occuption",
            "default": false,
            "class": "save"
        },
        {
            "title": "Button Group",
            "id": "button-group",
            "type": "button-group",
            "toggle": "occuption",
            "buttons": [
                {
                    "isPrimary": true,
                    "class": "red",
                    "id": "red",
                    "label": "Red"
                },
                {
                    "isPrimary": false,
                    "class": "blue",
                    "id": "red",
                    "label": "Blue"
                }
            ]
        },
        {
            "title": "Your father name",
            "id": "father-name",
            "toggle": "occuption",
            "type": "text",
            "attributeName": "name"
        },
        {
            "title": "City name",
            "toggle": "occuption",
            "id": "city-name",
            "type": "text",
            "attributeName": "name"
        },
        {
            "type": "select",
            "toggle": "occuption",
            "id": "gender",
            "title": "Country",
            "options": [
                {
                    "label": "India",
                    "value": "india"
                },
                {
                    "label": "USA",
                    "value": "usa"
                }
            ]
        },
        {
            "type": "radio",
            "title": "Gender",
            "id": "gender",
            "label": "Gender",
            "options": [
                {
                    "label": "Male",
                    "value": "m"
                },
                {
                    "label": "Female",
                    "value": "f"
                }
            ]
        },
        {
            "type": "range",
            "id": "volume",
            "title": "Volume",
            "min": 1,
            "max": 100
        }
    ]
}